{
  "AWSEBDockerrunVersion":  2,
  "ContainerDefinitions": [
    {
      "name": "client",
      "image":  "suak2004/multi-client",
      "hostname": "client",
      "essential":  false
    },
    {
      "name": "server",
      "image":  "suak2004/multi-server",
      "hostname": "api",
      "essential":  false
    },
    {
      "name": "worker",
      "image":  "suak2004/multi-worker",
      "hostname": "worker",
      "essential":  false
    },
    {
      "name": "nginx",
      "image":  "suak2004/multi-nginx",
      "hostname": "nginx",
      "essential":  true,
      "portMappings": [
        {
        "hostPort":80,
        "containerPort":80
        }
      ],
      "links":  ["client", "server"]
    }
  ]
}
